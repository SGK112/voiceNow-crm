{
  "expo": {
    "name": "VoiceFlow AI",
    "slug": "voiceflow-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": ["voiceflow-ai", "com.googleusercontent.apps.710258787879-732ell2g9g0llo41uispncfkpqr4qlf2"],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.voiceflow.ai",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "We need microphone access to make AI-powered voice calls on your behalf",
        "NSContactsUsageDescription": "We need access to your contacts to identify callers and personalize AI responses",
        "NSPhoneCallHistoryUsageDescription": "We need to detect missed calls so our AI can call them back automatically",
        "NSFaceIDUsageDescription": "Use Face ID to quickly and securely sign in to your account",
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#5865F2",
      "sounds": ["./assets/notification.wav"],
      "androidMode": "default",
      "androidCollapsedTitle": "Aria AI"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#5865F2",
          "sounds": ["./assets/notification.wav"]
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0b"
      },
      "package": "com.voiceflow.ai",
      "permissions": [
        "READ_PHONE_STATE",
        "READ_CALL_LOG",
        "CALL_PHONE",
        "READ_SMS",
        "SEND_SMS",
        "RECEIVE_SMS",
        "READ_CONTACTS",
        "INTERNET",
        "RECORD_AUDIO",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK"
      ],
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "aabe7aff-e3e7-41f2-9a66-e51bf6923d85"
      }
    }
  }
}
